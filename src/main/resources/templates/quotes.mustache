{{> header}}
<div class="container">
    <h1>Quotes</h1>
    <table class="table table-striped">
        <thead>
        <tr>
            <th scope="col">Number</th>
            <th scope="col">Author</th>
            <th scope="col">Quote</th>
            <th scope="col">Timestamp</th>
            <th scope="col">Link</th>
        </tr>
        </thead>
        <tbody>
    {{#quotes}}
        <tr>
            <th scope="row">{{id}}</th>
            <td>{{name}}</td>
            <td>{{#quote}}{{quote}}{{#image}}<br/>{{/image}}{{/quote}}{{#image}}<img src="{{image}}" height="50" width="50">{{/image}}</td>
            <td class="dateable">{{timestamp}}</td>
            <td><a href="https://discordapp.com/channels/740999022340341791/{{channelId}}/{{messageId}}">jump</a></td>
        </tr>
    {{/quotes}}
        </tbody>
    </table>
</div>
{{> dateable}}
{{> footer}}