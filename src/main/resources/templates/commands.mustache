{{> header}}
<div class="container-fluid">
    {{#commands}}
    <div class="card">
        <div class="card-body">
            <h5 class="card-title">{{name}}</h5>
            <p class="card-text">
                {{#aliases}}
                Aliases: {{aliases}}<br/>
                {{/aliases}}
                {{#requiredRole}}
                Required Role: {{requiredRole}}<br/>
                {{/requiredRole}}
                Help:
            </p>
            {{#modHelp}}
                <pre style="white-space: pre-wrap;">{{modHelp}}</pre>
            {{/modHelp}}
            {{^modHelp}}
                <pre style="white-space: pre-wrap;">{{help}}</pre>
            {{/modHelp}}
        </div>
    </div>
    {{/commands}}
</div>
{{> footer}}